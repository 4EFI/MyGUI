#include "MenuController.h"

int main()
{
    sf::RenderWindow window(sf::VideoMode(1000, 700), "Test");

    Button button;

    while(true)
    {
        sf::Event event;
        while (window.pollEvent(event))
        {
            if (event.type == sf::Event::Closed) window.close();
        }

        window.clear();

        button.Show(window);
        if(button.IsClicked)
            printf("Ye\n");

        window.display();
    }
}
